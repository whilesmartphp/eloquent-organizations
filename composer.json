{
  "name": "whilesmart/eloquent-organizations",
  "description": "Organization management package for Laravel applications",
  "package-version": "1.0.0",
  "keywords": [
    "laravel",
    "organization",
    "php",
    "package"
  ],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Whilesmart Team"
    }
  ],
  "require": {
    "php": "^8.2",
    "laravel/framework": "^11.0|^12.0",
    "whilesmart/eloquent-roles": "dev-main",
    "cviebrock/eloquent-sluggable": "^12.0",
    "zircote/swagger-php": "^5.0"
  },
  "require-dev": {
    "laravel/pint": "^1.25",
    "orchestra/testbench": "^9.0|^10.0",
    "phpunit/phpunit": "^10.0|^11.0"
  },
  "suggest": {
    "whilesmart/workspaces": "For workspace integration support"
  },
  "autoload": {
    "psr-4": {
      "Whilesmart\\Organizations\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Workbench\\App\\": "workbench/app/",
      "Whilesmart\\Organizations\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Whilesmart\\Organizations\\OrganizationsServiceProvider"
      ]
    }
  },
  "scripts": {
    "pint": [
      "./vendor/bin/pint"
    ],
    "pint:test": [
      "./vendor/bin/pint --test"
    ]
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
